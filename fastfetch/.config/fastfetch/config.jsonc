//  https://github.com/m3tozz/FastCat
{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "source": "$HOME/.config/fastfetch/ascii.txt",
        "padding": {
            "top": 1,
            "right": 1
        }
    },
    "display": {
        "separator": " | "
    },
    "modules": [
        "break",
        {
            "type": "title",
            "key": "  User ",
            "keyColor": "blink_",
            "format": "{user-name}@{host-name}"
        },
        {
            "type": "os",
            "key": "  OS   ",
            "keyColor": "33"
        },
        {
            "type": "shell",
            "key": "  SH   ",
            "keyColor": "33"
        },
        {
            "type": "memory",
            "key": "  MEM  ",
            "keyColor": "33"
        },
        {
            "type": "cpu",
            "key": "  CPU  ",
            "keyColor": "33"
        },
        {
            "type": "colors",
            "keyColor": "33",
            "key": "  TERM ",
            "symbol": "circle",
            "block": {
                "width": 10
            }
        }
    ]
}